{
  "name": "Youtube Tabs",
  "description": "Tidy up that Youtube subscription list with tab groups",
  "version": "1.1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": ["content.js", "iro.min.js"],
      "css": ["tabs.css"]
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}