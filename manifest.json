{
  "name": "Youtube Tabs",
  "description": "Tidy up your Youtube subscription list with tab organization features",
  "version": "1.5.2",
  "manifest_version": 3,
  "icons": {
    "128": "icon.png",
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["iro.min.js", "youtube-tabs.js"],
      "css": ["tabs.css"],
      "html": ["pop-up.html"]
    }
  ]
}