{
  "name": "Youtube Tabs",
  "description": "Tidy up your Youtube subscription list with tab organization features",
  "version": "1.5.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png",
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["iro.min.js", "content.js"],
      "css": ["tabs.css"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "grant@geekoverdrive.com"
    }
  }
}